package /nginx {

   rule nginx {
        (ip:sip,_^2,chars:timestamp<[,]>,http/request",chars:status,chars:size,chars:referer",http/agent",_")
        
   }

  rule flb_nginx {
        (
            json | take(log) | json_unescape() | (
                ip:sip,
                2*_,
                time:recv_time<[,]>,
                http/request",http/status,digit,chars",
                http/agent",_"
            )  
        )
  }
}
