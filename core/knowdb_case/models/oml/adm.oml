name : /oml/example/simple
rule : /example/*
---
recv_time  : time =   Time::now();
occur_time : time =  take(option:[time]) ;
src_ip     : ip   =  take(option:[src-ip,sip,source-ip] );
src_ip_int : digit = read(src_ip) | to_ip4_int ;
from_zone: chars = select zone from zone_data
            where  start_ip_int < read(src_ip_int) and  end_ip_int >= read(src_ip_int) ;
