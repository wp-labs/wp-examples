package /example/simple {
   rule nginx {
        (ip:sip,2*_,time:recv_time<[,]>,http/request",http/status,digit,chars",http/agent",_")
   }
    rule syslog_nginx {
        (
            _<<,>>,
            time,
            chars,
            chars\:,
            ip:sip,
            2*_,
            time:recv_time<[,]>,
            http/request",
            http/status,
            digit,
            chars",
            http/agent",
            _"
        )
    }
}
